@using CSSPWebTools.Views.Shared.Resources;
@using CSSPWebTools.Views.MWQM.Resources;

<div>
    <ul class="list-inline">
        <li><div class="h4">@MWQMViewRes.StatisticsData</div></li>
        <li>
            <div class="blockMargin dropdown">
                <a class="dropdown-toggle" id="MenuLegendTable" data-toggle="dropdown" aria-expanded="false">
                    <span class="glyphicon glyphicon-list-alt"></span>&nbsp; <span title="@MWQMViewRes.Abbreviations">@MWQMViewRes.Abbreviations_abbr</span>
                </a>
                <ul class="dropdown-menu small" role="menu" aria-labelledby="MenuLegendTable">
                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#">@MWQMViewRes.FecalColiforms_abbr: @MWQMViewRes.FecalColiforms</a>
                    </li>
                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#">@UnitViewRes.PPT: @MWQMViewRes.Salinity</a>
                    </li>
                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#">@UnitViewRes.Celcius_abbr: @MWQMViewRes.Temperature</a>
                    </li>
                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#">@MWQMViewRes.Depth_abbr: @MWQMViewRes.Depth</a>
                    </li>
                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#">@MWQMViewRes.GeometricMean_abbr: @MWQMViewRes.GeometricMean</a>
                    </li>

                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#"> @MWQMViewRes.Median_abbr: @MWQMViewRes.Median</a>
                    </li>
                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#"> @MWQMViewRes.Percentile90_abbr: @MWQMViewRes.Percentile90</a>
                    </li>
                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#"> &gt;43 (%): @MWQMViewRes.Percentagegt43</a>
                    </li>
                    <li role="presentation">
                        <a class="" role="menuitem" tabindex="-1" href="#"> &gt;260 (%): @MWQMViewRes.Percentagegt260</a>
                    </li>
                </ul>
            </div>
        </li>
    </ul>
    <div class="paraBlock">
        <ul class="list-inline">
            <li>
                &nbsp;
                <label class="checkbox-inline">
                    <input type="checkbox" checked="checked" data-colnumb="3" title="@MWQMViewRes.Salinity" /> (@UnitViewRes.PPT)
                </label>
            </li>
            <li>
                <label class="checkbox-inline">
                    <input type="checkbox" checked="checked" data-colnumb="4" title="@MWQMViewRes.Temperature" /> (@UnitViewRes.Celcius_abbr)
                </label>
            </li>
            <li>
                <label class="checkbox-inline">
                    <input type="checkbox" checked="checked" data-colnumb="5" />@MWQMViewRes.pH

                </label>
            </li>
            <li>
                <label class="checkbox-inline" title="@MWQMViewRes.Abbreviations: @MWQMViewRes.Depth_abbr">
                    <input type="checkbox" checked="checked" data-colnumb="6" />@MWQMViewRes.Depth (@UnitViewRes.Meters_abbr)

                </label>
            </li>
            <li>
                <label class="checkbox-inline" title="@MWQMViewRes.GeometricMean">
                    <input type="checkbox" checked="checked" data-colnumb="7" />@MWQMViewRes.GeometricMean_abbr

                </label>
            </li>
            <li>
                <label class="checkbox-inline" title="@MWQMViewRes.Median">
                    <input type="checkbox" checked="checked" data-colnumb="8" />@MWQMViewRes.Median_abbr

                </label>
            </li>
            <li>
                <label class="checkbox-inline" title="@MWQMViewRes.Percentile90">
                    <input type="checkbox" checked="checked" data-colnumb="9" />@MWQMViewRes.Percentile90_abbr

                </label>
            </li>
            <li>
                <label class="checkbox-inline">
                    <input type="checkbox" checked="checked" data-colnumb="10" />&gt;43

                </label>
            </li>
            <li>
                <label class="checkbox-inline">
                    <input type="checkbox" checked="checked" data-colnumb="11" />&gt;260
                </label>
            </li>
            <li>
                <label>
                    <button class="jbHelpDocShowDialog btn btn-link" data-dockey="mwqmsitestatisticaldata" data-title="@MWQMViewRes.StatisticsData"><span class="glyphicon glyphicon-info-sign"></span></button>
                </label>
            </li>
        </ul>
    </div>
    <div id="MWQMSiteTableDataID">

    </div>
    <script type="text/javascript">
        $(function () {
            cssp.MWQMSite.DrawTable();
        });
    </script>
</div>
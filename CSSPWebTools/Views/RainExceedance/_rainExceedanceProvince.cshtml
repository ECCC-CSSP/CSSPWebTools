@using CSSPWebTools.Views.RainExceedance.Resources;
@using CSSPWebTools.Models;
@using CSSPEnumsDLL.Enums;
@using CSSPEnumsDLL.Services;
@using CSSPModelsDLL.Models;
@using CSSPWebTools.Controllers;

@{
    List<TVItemModel> TVItemModelProvinceList = (List<TVItemModel>)ViewBag.TVItemModelProvinceList;
}

<h4>@RainExceedanceViewRes.ApplyToProvince</h4>
<ul class="list-unstyled">
    @if (TVItemModelProvinceList.Count == 0)
            {
        <li>@RainExceedanceViewRes.Empty</li>
    }
    else
    {
        foreach (TVItemModel TVItemModel in TVItemModelProvinceList)
        {
            <li>@TVItemModel.TVText</li>
        }
    }
</ul>
